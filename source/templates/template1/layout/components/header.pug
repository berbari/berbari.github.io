mixin header

  +b.HEADER.header
    +e.navbar
      .container
        +e.A.logo(href='/')
          img(src=assets.logo, alt='execyoution')
        +e.BUTTON.navbar--toggle.navbar-toggle(type='button')
          span.icon-bar
          span.icon-bar
          span.icon-bar
        +e.UL.navbar--nav.nav.navbar-nav.navbar-right
          for link in navlinks
            li
              a(href=link.href)
                span !{link.text}
